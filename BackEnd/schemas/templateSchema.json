{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "id": 1,
            "wall": "A1, B2, C3",
            "height": 25,
            "width": 25,
            "user_id": 1
        }
    ],
    "required": [
        "wall",
        "height",
        "width"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "wall": {
            "$id": "#/properties/wall",
            "type": "string",
            "title": "The wall schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "A1, B2, C3"
            ]
        },
        "height": {
            "$id": "#/properties/height",
            "type": "integer",
            "title": "The height schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                25
            ]
        },
        "width": {
            "$id": "#/properties/width",
            "type": "integer",
            "title": "The width schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                25
            ]
        },
        "user_id": {
            "$id": "#/properties/user_id",
            "type": "integer",
            "title": "The user_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        }
    },
    "additionalProperties": true
}